@compiletime function createBuilder()
    createBuilder(UNIT_BEAST_BUILDER, UnitIds.peasant)
        ..setNameEnhance("S", "Beast Builder")
        ..setModelFile(Units.kobold1)
        ..setIconGameInterface(Icons.bTNKobold)
        ..setUnitSoundSet(Soundsets.kobold)
        ..setStructuresBuilt(commaList(
            BUILDING_GNOLL,
            BUILDING_TAUREN,
            BUILDING_THUNDER_LIZARD,
            BUILDING_CENTAUR,
            BUILDING_HYDRA,
            BUILDING_REXXAR,
            BUILDING_CHEN
        ))
