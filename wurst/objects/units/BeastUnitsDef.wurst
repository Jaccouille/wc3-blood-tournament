package BeastUnitsDef

// Standard libs imports:
import Assets
import ChannelAbilityPreset

// Local imports:
import LocalObjectIDs
import UnitObjEditingCommon
import UnitTooltipGenerator


@compiletime function createBeastUnitsDef()
    let tooltip = new UnitTooltipGenerator("Footman Desc")
    createSpawnedUnit(UNIT_GNOLL, UnitIds.gnoll1, BUILDING_GNOLL)
        ..registerTooltipGenerator(tooltip)
        ..tooltipStartListen()
        ..setAttack1DamageBase(14)
        ..setHitPointsMaximumBase(100)
        ..setNameEnhance("Q", "Gnoll")
        ..setAutoPointValue()
        ..tooltipStopListen(true)
        ..setSizeSmall()
        ..setPriority(2)
        ..setBuildingGoldCost(100)
        ..setUpgradeTo(commaList(BUILDING_GNOLL_UPGRADE))
        ..setUnitsTrained(commaList(UNIT_GNOLL, UNIT_RAZORMAN, UNIT_WOLF, UNIT_FURBOLG, UNIT_SASQUATCH))
        ..setBuildingName("Q", "5x Lesser Beasts")

        ..setModelFileExt(Units.gnoll1)
        ..setIconGameInterfaceExt(Icons.bTNGnoll)
        ..setUnitSoundSetExt(Soundsets.gnoll)

    createSpawnedUnit(UNIT_RAZORMAN, UnitIds.razormanebrute)
