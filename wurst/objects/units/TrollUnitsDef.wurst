@compiletime function createBuilder()
    createBuilder(UNIT_TROLL_WORKER, UnitIds.peasant)
        ..setNameEnhance("D", "Troll Builder")
        ..setModelFile(LocalUnits.gruntTrollIsle)
        ..setIconGameInterface(Icons.bTNHeadhunter)
        ..setUnitSoundSet(Soundsets.batTroll)
        ..setStructuresBuilt(commaList(
            BUILDING_HEADHUNTER,
            BUILDING_SHADOWHUNTER,
            BUILDING_AXE_THROWER,
            BUILDING_DIRE_TROLL,
            BUILDING_WITCH_DOCTOR,
            BUILDING_PYTHON_WARRIORES,
            BUILDING_VOLJIN,
            BUILDING_ZULJIN,
            BUILDING_DIREHORN,
            BUILDING_WARRIOR_TROLL,
            BUILDING_BIG_DINO
        ))
