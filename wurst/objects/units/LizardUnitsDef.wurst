@compiletime function createLizardUnitsDef()
    let tooltip = new UnitTooltipGenerator("Footman Desc")
    createSpawnedUnit(UNIT_LIZARD_WARRIOR, UnitIds.footman, BUILDING_LIZARD_WARRIOR)
        ..registerTooltipGenerator(tooltip)
        ..tooltipStartListen()
        ..setAttack1DamageBase(14)
        ..setHitPointsMaximumBase(100)
        ..setNameEnhance("Q", "Warrior")
        ..setAutoPointValue()
        ..tooltipStopListen(true)
        ..setSizeSmall()
        ..setPriority(2)
        ..setBuildingGoldCost(100)
        ..setUpgradeTo(commaList(""))

        ..setModelFileExt(LocalUnits.lizWarrior)
        ..setIconGameInterfaceExt(LocalIcons.bTNlizicon)
        ..setUnitSoundSetExt(Soundsets.windSerpent)

