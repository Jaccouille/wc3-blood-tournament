@compiletime function createBuilder()
    createBuilder(UNIT_LIZARD_WORKER, UnitIds.peasant)
        ..setNameEnhance("A", "LizardMan Builder")
        ..setModelFile(LocalUnits.lizSlaveHd16)
        ..setIconGameInterface(LocalIcons.bTNlizlaveicon2)
        ..setUnitSoundSet(Soundsets.nagaMyrmidon)
        ..setStructuresBuilt(commaList(
            BUILDING_LIZARD_WARRIOR,
            BUILDING_LIZARD_RAPTOR_RIDER,
            BUILDING_LIZARD_SPEARMAN,
            BUILDING_LIZARD_KNIGHT,
            BUILDING_LIZARD_WARLORD,
            BUILDING_LIZARD_ARCHER,
            BUILDING_LIZARD_DRAGON_PRIEST,
            BUILDING_LIZARD_WITCH,
            BUILDING_LIZARD_ROYAL_GUARD,
            BUILDING_LIZARD_ASSASSIN,
            BUILDING_LIZARD_HIGH_ARCHER
        ))
